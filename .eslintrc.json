{
  "extends": "airbnb-base",
  "rules": {
    "space-before-function-paren": [0],
    "vars-on-top": [0],
    "prefer-arrow-callback": [0],
    "no-var": [0],
    "object-shorthand": [0],
    "func-names": [0],
    "no-param-reassign": [0],
    "max-len": [0],
    "radix": [0]
  },
  "env": {
    "browser": false,
    "node": true
  }
}